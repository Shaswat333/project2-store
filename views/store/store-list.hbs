<h2>List of all Stores</h2>

{{#each allStore}}
<div>
  <p>owner:{{owner}}</p>
  <p>nameOfStore: {{nameOfStore}}</p>
  <p>
    description:{{description}}
  </p>
  <p>location: {{location}}</p>
  <p>rating : {{rating}}</p>

<div class="card" style="width: 18rem;">
  <img src="data:image/png;base64," class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{nameOfStore}}</h5>
    <p class="card-text">{{description}}</p>
    <a href="/store/{{_id}}" class="btn btn-primary">See details</a>
  </div>
</div>
{{/each}}
